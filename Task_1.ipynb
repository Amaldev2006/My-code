   "source": [
    "c=float(input(\"Enter temperature:\"))\n",
    "ask_1=input(\"is this in (C)elsius or (F)ahrenheit:\")\n",
    "if ask_1== 'C':\n",
    "    fahrenheit=(9/5*c)+32\n",
    "    print(f\"{c} celsius is {fahrenheit} fahrenheit\")\n",
    "elif ask_1=='F':\n",
    "    celsius=5/9*(c-9)\n",
    "    print(f\"{c} fahrenheit is {celsius} celsius\")"
   ]
